class SC1 extends Lv2.Plugin {
    constructor(attack,threshold,makeup) {

      local sc1State = Lv2.State({
          ["attack"] = attack.tostring(),
		  ["threshold"] = threshold.tostring(),
		  ["makeup_gain"] = makeup.tostring(),
      })

      base.constructor("http://plugin.org.uk/swh-plugins/sc1", sc1State)
    }
}
